    "babel-jest": "^26.5.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "execa": "^5.0.0",
    "jest": "^26.5.0",
    "prettier": "^2.1.2",
    "qs": "^6.9.4",
    "svg-sprite-loader": "^5.0.0",
    "vue-jest": "^3.0.4",
