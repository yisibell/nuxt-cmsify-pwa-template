{
  "name": "nuxt-cmsify-pwa-template",
  "version": "1.0.0",
  "description": "A Progressive Web App template based on nuxt.js",
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "keywords": [
    "vue",
    "nuxt",
    "cmsify pwa template",
    "cms",
    "pwa",
    "ssr",
    "vuetify",
    "pm2",
    "koa"
  ],
  "scripts": {
    "pm2:start": "pm2 start",
    "pm2:stop": "pm2 stop NuxtCmsifyPwaTemplate",
    "pm2:restart": "pm2 restart NuxtCmsifyPwaTemplate",
    "pm2:reload": "pm2 reload NuxtCmsifyPwaTemplate",
    "pm2:delete": "pm2 delete NuxtCmsifyPwaTemplate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest",
    "start": "node ./scripts/app.js",
    "build": "node ./scripts/build.js --theme=default",
    "build:all": "node ./scripts/build.js --all",
    "dev": "node ./scripts/dev.js --theme=default"
  },
  "dependencies": {
    "@aidol/svg-icon": "^1.2.0",
