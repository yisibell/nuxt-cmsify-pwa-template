    "core-js": "^3.6.5",
    "defu": "^3.2.2",
    "koa": "^2.13.1",
    "nuxt": "^2.14.12",
    "vue-i18n": "^8.22.2",
    "vue-toasted": "^1.1.28"
  },
  "devDependencies": {
    "@nuxtjs/composition-api": "^0.17.0",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.3",
    "@nuxtjs/vuetify": "^1.11.2",
